<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="5" id="uk.org.tiro.android.Inscriptions" version="1.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="SQLitePlugin">
        <param name="android-package" value="org.pgsqlite.SQLitePlugin" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
    </feature>
    <name>Inscriptions</name>
    <description>
    	Application to view Ancient Inscriptions.
    </description>
    <author email="inscriptions@tiro.org.uk" href="http://tiro.org.uk/">
    	Richard Palmer
    </author>
    <content src="index.html" />
    <access origin="*" />
    <preference name="orientation" value="landscape" />
    <preference name="SplashScreen" value="" />
</widget>
